# ::id lpp_de.24 
# ::snt Der kleine Prinz sah sich nach einem Sitzplatz um , doch der ganze Planet wurde von dem herrlichen Hermelinmantel bedeckt .
(s / schauen-01
    :ARG0 (p / Prinz
              :mod (k / klein))
    :ARG1 (s2 / Sitzplatz)
    :concession (b / bedecken-01
                    :ARG1 (p2 / Planet
                              :mod (c8 / ganze))
                    :ARG2 (c2 / Mantel
                              :mod (c3 / Hermelin)
                              :mod (c6 / herrliche))))

