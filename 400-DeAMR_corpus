# ::id lpp_de.1
# ::snt So könnt ihr euch vorstellen , wie überrascht ich war , als ich bei Sonnenaufgang von einer leisen , lustigen Stimme geweckt wurde .
(c / cause-01
     :ARG1 (k / können-01
                :ARG1 (v / vorstellen-01
                           :ARG0 (s / sie)
                           :ARG1 (u / überraschen-01
                                      :ARG0 (w / wecken-01
                                                 :ARG0 (s2 / Stimme
                                                            :mod (l / leise)
                                                            :mod (l2 / lustige)
                                                            :ARG1 i)
                                                 :time (s3 / Sonnenaufgang))
                                      :ARG1 (i / ich)))))

# ::id lpp_de.2 
# ::snt Er antwortete mir : " Das macht nichts . "
(a / antworten-01
    :ARG0 (e / er)
    :ARG1 (i / ich)
    :ARG2 (m / machen-01
              :ARG0 (d / das)
              :ARG1 (n / nichts)))

# ::id lpp_de.3
# ::snt Zeichne mir ein Schaf .
(z / zeichnen-01
    :mode imperative
    :ARG0 (d / du)
    :ARG1 (s / Schaf)
    :ARG2 (i / ich))

# ::id lpp_de.4 
# ::snt Da , wo ich lebe , ist alles sehr klein .
(k / klein
    :domain (a / alles)
    :degree (s / sehr)
    :location (l / leben-01
                  :ARG0 (i / ich)))

# ::id lpp_de.5 
# ::snt Das ist ja lustig !
(l / lustig
    :mode surprised
    :domain (d / das))

# ::id lpp_de.6 
# ::snt Von welchem Planeten stammst du ?
(s / stammen-01
    :ARG1 (d / du)
    :ARG2 (P / Planet
              :location (a / amr-unknown)))

# ::id lpp_de.7 
# ::snt Wo kommst du her , kleiner Kerl ?
(s / herkommen-01
    :ARG1 (d / du
              :mod (k / Kerl
                       :degree (k2 / kleine)))
    :ARG2 (a / amr-unknown))

# ::id lpp_de.8 
# ::snt Wenn ein Astronom einen von ihnen entdeckt , so gibt er ihm eine Nummer anstelle eines Namens .
(h / have-condition-91
     :ARG1 (g / geben-01
               :ARG0 (a / Astronom)
               :ARG1 (i / instead-of-91
                          :ARG1 (n / Nummer)
                          :ARG2 (n2 / Name)))
     :ARG2 (e / entdecken-01
               :ARG0 a
               :ARG1 (d / Ding
                          :quant 1
                          :ARG1-of (i2 / include-91
                                        :ARG2 (i3 / ihnen)))))

# ::id lpp_de.9 
